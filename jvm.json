{
  "servers": [
    {
      "port" : "${JMX_PORT}",
      "host" : "${SOURCE_HOST}",
      "alias": "tomcat",
      "queries": [
        {
          "outputWriters": [
            {
              "@class": "com.googlecode.jmxtrans.model.output.InfluxDbWriterFactory",
              "url": "${URL}",
              "database": "${DB_NAME}",
              "username": "${DB_USER}",
              "password": "${DB_PASS}",
              "tags" : {"instance" : "${ALIAS_NODE}"}
            }
          ],
          "obj": "Catalina:j2eeType=Servlet,name=*,WebModule=//localhost/,J2EEApplication=none,J2EEServer=none",
          "attr": ["errorCount",
                "processingTime",
                "requestCount"],
          "resultAlias": "tomcat.servlet.${APPLICATION}"
        }
      ],
      "numQueryThreads": 1
    }
  ]
}
